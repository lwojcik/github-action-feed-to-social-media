{
  "name": "github-action-feed-to-social-media",
  "version": "2.4.0",
  "private": true,
  "description": "Post latest RSS / Atom feed item to multiple social platforms",
  "main": "dist/index.js",
  "scripts": {
    "start": "npx ncc run ./src/index.ts",
    "build": "npx ncc build ./src/index.ts",
    "watch": "tsc --watch",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "test:cov": "jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "all": "npm run format && npm run lint && npm test && npm run build && npm run package"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/io": "^1.1.3",
    "@extractus/feed-extractor": "6.2.4",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@typescript-eslint/parser": "^6.1.0",
    "@vercel/ncc": "^0.36.1",
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "discord.js": "^14.11.0",
    "easy-bsky-bot-sdk": "^0.1.2",
    "eslint": "^8.45.0",
    "eslint-plugin-github": "^4.9.2",
    "eslint-plugin-jest": "^27.2.3",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "masto": "^5.11.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "twitter-api-v2": "^1.15.0",
    "typescript": "^5.1.6"
  }
}
